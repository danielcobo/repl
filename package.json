{
 "name": "@danielcobo/repl",
 "version": "1.0.0",
 "description": "JavaScript Browser REPL",
"source":"./src/index.html", 
"scripts": {
  "test": "jest --collectCoverage --runInBand",
  "testing": "jest --watch --collectCoverage --runInBand",
  "mutation": "npx stryker run",
  "docs": "docs",
  "patch": "git push && npm version patch && npm publish",
  "minor": "git push && npm version minor && npm publish",
  "major": "git push && npm version major && npm publish",
  "start": "parcel",
  "build": "parcel build",
  "rollup": "./node_modules/.bin/rollup -c",
  "prepublishOnly": "./node_modules/.bin/rollup -c"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/danielcobo/repl.git"
 },
 "keywords": [
  "repl",
  "javascript",
  "browser"
 ],
 "author": "Daniel Cobo (https://danielcobo.com/)",
 "license": "MIT",
 "bugs": {
  "url": "https://github.com/danielcobo/repl/issues"
 },
 "homepage": "https://github.com/danielcobo/repl#readme",
 "devDependencies": {
  "@danielcobo/docs": "^1.0.16",
  "@rollup/plugin-commonjs": "^20.0.0",
  "@rollup/plugin-node-resolve": "^13.0.4",
  "@stryker-mutator/core": "^5.4.1",
  "@stryker-mutator/jest-runner": "^5.4.1",
  "jest": "^27.2.5",
  "parcel": "^2.2.1",
  "rollup": "^2.56.3",
  "rollup-plugin-terser": "^7.0.2"
 },
 "dependencies": {
  "monaco-editor": "^0.31.1"
 }
}
